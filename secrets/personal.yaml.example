# Personal Secrets Template  
# Copy this to personal.yaml and edit with your actual values
# Run: cp personal.yaml.example personal.yaml && sops personal.yaml

# API Keys for your quickshell widgets and personal tools
api_keys:
  weather: "your_weather_api_key_here"
  github: "ghp_your_github_personal_access_token"
  openweather: "your_openweathermap_api_key" 
  translate: "your_google_translate_api_key"
  ai_service: "your_openai_or_anthropic_api_key"

# Social/Communication
social:
  discord_token: "your_discord_bot_token"
  telegram_token: "your_telegram_bot_token"

# Cloud services
cloud:
  aws_access_key: "your_aws_access_key"
  aws_secret_key: "your_aws_secret_key"
  digitalocean_token: "your_do_api_token"
  cloudflare_token: "your_cloudflare_api_token"

# Development
development:
  npm_token: "your_npm_publish_token"
  docker_registry_password: "your_private_registry_password"
  
# Personal finance/services
services:
  bitwarden_master_password: "your_bitwarden_master_password"
  email_app_password: "your_email_app_specific_password"