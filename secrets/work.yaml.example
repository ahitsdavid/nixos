# Work Secrets Template
# Copy this to work.yaml and edit with your actual values  
# Run: cp work.yaml.example work.yaml && sops work.yaml

# Work-specific API keys and credentials
work_apis:
  company_internal_api: "your_company_api_key"
  monitoring_service: "your_datadog_or_newrelic_key"
  ci_cd_token: "your_jenkins_or_gitlab_ci_token"
  
# Work VPN and network
network:
  company_vpn_config: |
    # Your work VPN configuration
  company_wifi: "work_wifi_password"
  
# Work services
services:
  company_email_password: "your_work_email_app_password"
  company_slack_token: "your_slack_bot_token"
  jira_api_token: "your_jira_api_token"
  confluence_token: "your_confluence_api_token"

# Development/DevOps credentials  
devops:
  kubernetes_config: |
    # Your k8s config with sensitive cluster info
  docker_registry_work: "work_docker_registry_password"
  terraform_cloud_token: "your_terraform_cloud_api_token"